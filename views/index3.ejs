


<%if(a.length==0){%>
<h1>No Employee</h1>
<%}else{%>
<h1>Employee Details</h1>
	<table border="1">
		<th>id</th>
		<th>name</th>
		<th>email</th>
		<th>contact</th>
		<th>city</th>
		<th>university</th>
		
		<th>Actions</th>

	<%for(let x of a){%>
		<tr>
			<td><%=x.id%></td>
			<td><%=x.name%></td>
			<td><%=x.email%></td>
			
			<td><%=x.contact%></td>
			<td><%=x.city%></td>
			
			<td><%=x.university%></td>
			
			<td>
				<a href="edit/<%=x.id%>">edit</a>
				<a href="delete/<%=x.id%>">delete</a>
			</td>
		</tr>
	<%}%>
	</table>
<%}%>


<style>
	input:invalid
	{
		border:2px solid red;
	}
</style>